{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Prevalnce of any long-term mental disoder",
    "data": {"url": "https://raw.githubusercontent.com/etra0014/assignment2/main/data/12mthdisorder.csv"
    },
    "mark": "bar",
    "encoding": {
      "x": {"field": "Age group"},
